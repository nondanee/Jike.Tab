{
	"name": "Jike.Tab",
	"description": "Override the new tab page with a blank one",
	"version": "0.2",
	"incognito": "split",
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"icons": {
		"16": "images/logo16.png",
		"48": "images/logo48.png",
		"128": "images/logo128.png"
	},
	"background": {
		"scripts": ["background.js"]
	},
	"manifest_version": 2,
	"permissions": ["*://app.jike.ruguoapp.com/*", "*://web.okjike.com/*", "webRequest", "webRequestBlocking"]
}